{
    "name": "esign/laravel-model-files",
    "description": "Associate files with your Laravel Models",
    "keywords": [
        "esign",
        "model-files"
    ],
    "homepage": "https://github.com/esign/laravel-model-files",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy.vanderhaegen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^8.74|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\ModelFiles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\ModelFiles\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\ModelFiles\\ModelFilesServiceProvider"
            ],
            "aliases": {
                "ModelFiles": "Esign\\ModelFiles\\Facades\\ModelFilesFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
