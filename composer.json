{
    "name": "esign/laravel-model-files",
    "description": "Associate files with your Laravel Models",
    "keywords": [
        "esign",
        "model-files"
    ],
    "homepage": "https://github.com/esign/laravel-model-files",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy.vanderhaegen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "esign/laravel-underscore-translatable": "^1.4",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^8.02|^9.0|^10.0",
        "phpunit/phpunit": "^10.1|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\ModelFiles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\ModelFiles\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
